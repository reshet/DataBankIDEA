<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:views="urn:import:com.mresearch.databank.client.views">
	<ui:style>
	
		.tableStyle table {
		    font-family: 'Trebuchet MS', Verdana, Arial, Helvetica;
		    font-size: 14px;
		    color: #4d4d4d;
		    border-collapse: collapse;
		}
		
		.tableStyle th {
		    text-align: right;
		    min-width: 100px;
		    font-weight: normal;
		    padding: 2px 5px;
		    border-right: #cccccc solid 1px;
		    /*border-bottom: #e5e5e5 solid 1px;*/
		}
		
		.tableStyle td {
		    color: #656565;
		    text-align: right;
		    border-right: #cccccc solid 1px;
		    border-bottom: #e5e5e5 solid 1px;
		    padding: 2px 5px;
		
		}
		
		.tableStyle tr > th:first-child, .tableStyle  tr > td:first-child  {
		    color: #4d4d4d;
		    text-align: left;
		    border-right: #cccccc solid 3px;
		    
		 
		}
		
		
		.analisys{
			text-align: right;
		
		}
		.text_f
		{
			font-weight: bold;
		}
		
		
		
		.cleaner table {
		    font-family: 'Trebuchet MS', Verdana, Arial, Helvetica;
		    font-size: 14px;
		    color: #4d4d4d;
		    border-collapse: collapse;
		}
		
		.cleaner th {
		    text-align: right;
		    min-width: 100px;
		    font-weight: normal;
		    padding: 2px 5px;
		    border-right: #cccccc solid 0px;
		    /*border-bottom: #e5e5e5 solid 1px;*/
		}
		
		.cleaner td {
		    color: #656565;
		    text-align: right;
		    border-right: #cccccc solid 0px;
		    border-bottom: #e5e5e5 solid 0px;
		    padding: 2px 5px;
		
		}
		
		.cleaner tr > th:first-child, .cleaner  tr > td:first-child  {
		    color: #4d4d4d;
		    text-align: left;
		    border-right: #cccccc solid 0px;
		   
		   
		 
		}
		
		 .pad_left
		 {
			padding-left: 15px;			
	
		}  
		
		
		
		
	</ui:style>
	
	<g:HTMLPanel styleName="{style.pad_left}">
		<h2>Сравнение переменных</h2>
		<g:HTMLPanel ui:field="main_html">
			<g:FlexTable ui:field="compareTbl" styleName="{style.tableStyle}"></g:FlexTable>
			<br/>
		</g:HTMLPanel>		
	</g:HTMLPanel>
	
	
</ui:UiBinder> 