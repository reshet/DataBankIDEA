<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.category
		{
			font-weight: bold;
			font: 15px;
			vertical-align: text-top;
		}
	</ui:style>
	<g:HTMLPanel>
		<table>
			<tr>
				<td>
					<g:Label ui:field="field_name" styleName="{style.category}"></g:Label>
				</td>
				<td>
					<g:TextBox ui:field="contents"></g:TextBox>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 