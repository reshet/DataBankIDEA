<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel ui:field="panel">
		<table>
			<tr>
				<td>Логин:</td>
				<td><g:TextBox ui:field="login_box"></g:TextBox></td>
			</tr>
			<tr>
				<td>Пароль:</td>
				<td><g:PasswordTextBox  ui:field="pswd_box"></g:PasswordTextBox></td>
			</tr>
			<tr>
				<td>
					<g:Button ui:field="enterBtn" text="Вход"></g:Button>
					
				</td>
				<td>
					<g:Button ui:field="authGoogleBtn" text="Вход через Google"></g:Button>
					
				</td>
			</tr>
			
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 