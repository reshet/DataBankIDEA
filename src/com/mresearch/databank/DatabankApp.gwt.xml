<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='databanknewest'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>
    <!--   <inherits name="com.google.gwt.logging.Logging"/> -->
    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <!--   <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
    <inherits name='com.google.gwt.user.theme.clean.Clean'/>
    <!--      <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->

    <!--   <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
    <!-- Other module inherits                                      -->
    <inherits name="com.google.gwt.xml.XML"/>
    <inherits name="com.google.gwt.i18n.I18N"/>
<!--    <inherits name="com.google.gwt.i18n.CldrLocales"/>-->
    <extend-property name="locale" values="ru_RU, uk_UA"/>
    <!--<set-property name="locale" value="ru_RU"/>-->
    <set-property-fallback name="locale" value="uk_UA"/>
    <set-property name="user.agent" value="safari, gecko1_8, ie9" />

<!--
    <define-property name="user.agent" values="igecko,gecko1_8,safari"></define-property>
-->

    <inherits name='org.zenika.widget.DatePickerModule'/>
    <inherits name="org.moxieapps.gwt.highcharts.Highcharts"/>

    <!-- Include GWTUpload library -->
    <inherits name="gwtupload.GWTUpload"/>
    <inherits name='com.smartgwt.SmartGwt'/>
    <inherits name="com.google.api.gwt.oauth2.OAuth2" />
    <inherits name="com.google.gwt.json.JSON"/>

    <stylesheet src="CoolStyles.css"/>
    <entry-point class='com.mresearch.databank.client.DatabankApp'/>
    <source path='client'/>
    <source path='shared'/>

</module>
